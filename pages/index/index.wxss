/**index.wxss**/
.index {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.i-menu{
  flex: none;
  display: flex;
  flex-direction: row;
  justify-content:space-between;
  background-color: #fff;
  border-bottom: 1px solid #f0f0f0;
  height: 80rpx;
}
.i-menu-btn{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  width: 50%;
  font-size: 32rpx;
}
.selected{
  color: #2fcc85;
  border-bottom: 1px solid #2fcc85;
}
.pageA,.pageB{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.timeA_text{
  margin-top: 160rpx;
  padding-left: 50rpx;
  padding-right: 50rpx;
  padding-bottom: 40rpx;
  border: 10rpx double #2fcc85;
}
.pageA_but{
  margin-top: 86rpx;
}
.s_s{
  display: flex;
  flex-direction: row;
}
.start,.stop,.hold{
  margin: 32rpx;
  margin-top: 16rpx;
}
.inputA{
  text-align: center;
  margin-top: 90rpx;
  border: 1px solid #000;
}
.modalView{
  display: flex;
  flex-direction: column;
  color: #000;
}
.modalInputA{
  width: 100%;
  border: 1px solid #000;
}