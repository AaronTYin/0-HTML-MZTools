    <image class='iconImg' src='../../icons/PhoneInfo.png'></image>
    <view>手机信息</view>
  </button>
</view>

<view class='funButGroup2'>
  <button class='iconBtn' bindtap='toWeather'>
    <image class='iconImg' src='../../icons/Weather.png'></image>
    <view>天气</view>
  </button>
  <button class='iconBtn' bindtap='toMap'>
    <image class='iconImg' src='../../icons/Map.png'></image>
