  </button>

  <button class='iconBtn' open-type='launchApp' app-parameter='wechat' binderrror='launchAppError'>
    <image class='iconImg' src='../../icons/WeChat.png'></image>
    <view>微信</view>
  </button>
</view>

<view class='funButGroup3'>
  <button class='iconBtn' bindtap='toFlashlight'>
    <image class='iconImg' src='../../icons/Flashlight.png'></image>
    <view>手电筒</view>
  </button>
  <button class='iconBtn' bindtap='toClipboard'>
    <image class='iconImg' src='../../icons/Clipboard.png'></image>
