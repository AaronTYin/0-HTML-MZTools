<view class='funButGroup1'>
  <button class='iconBtn' bindtap='toTimeTask'>
    <image class='iconImg' src='../../icons/TimeTask.png'></image>
    <view>时间打卡</view>
  </button>
  <button class='iconBtn' bindtap='toCalculator'>
    <image class='iconImg' src='../../icons/Calculator.png'></image>
    <view>计算器</view>
  </button>
  <button class='iconBtn' bindtap='toLifePlan'>
    <image class='iconImg' src='../../icons/LifePlan.png'></image>
